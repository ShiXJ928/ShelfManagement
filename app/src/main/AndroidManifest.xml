<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zq.sm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".acty.MainActy">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".acty.LendEquipmentListActy" />
        <activity android:name=".acty.OverdueEquipmentActy" />
        <activity android:name=".acty.TypeEquipmentListActy" />
        <activity android:name=".acty.SettingActy" />
    </application>
</manifest>
